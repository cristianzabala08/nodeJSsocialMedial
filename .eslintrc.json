{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "semi": "error",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "spaced-comment": "off",
    "object-curly-newline": "off"
  }
}
